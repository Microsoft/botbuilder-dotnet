
{
    "$schema": "https://raw.githubusercontent.com/microsoft/botbuilder-dotnet/chrimc/form/schemas/sdk.schema",
    "$kind": "Microsoft.OnAssignEntity",
    "property": "Directions",
    "entity": "DirectionsEntity",
    "actions": [
        {
            "$kind": "Microsoft.IfCondition",
            "condition": "!turn.dialogevent.value.expected",
            "actions":[               
                {
                    "$kind": "Microsoft.IfCondition",
                    "condition": "!$Directions",
                    "actions":[
                        {
                            "$kind": "Microsoft.SendActivity",
                            "activity": "@{unexpectedPropertySet('Directions', @DirectionsEntity)}"
                        }
                    ],
                    "elseActions":[
                        {
                            "$kind": "Microsoft.SendActivity",
                            "activity": "@{unexpectedPropertyChange('Directions', @DirectionsEntity, $Directions)}"
                        }
                    ]
                }                
            ]           
        },
        {
            "$kind": "Microsoft.SetProperty",
            "property": "$Directions",
            "value": "@DirectionsEntity"
        }
    ]
}
