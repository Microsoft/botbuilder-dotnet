{
    "$kind": "Microsoft.AdaptiveDialog",
  "generator": {
    "$kind": "Microsoft.ResourceMultiLanguageGenerator",
    "resourceId": "root.lg",
    "languagePolicy": {
      "": [ "fr-fr", ""]
    }
  },
    "triggers": [
        {
            "$kind": "Microsoft.OnBeginDialog",
            "actions": [
                {
                    "$kind": "Microsoft.SendActivity",
                    "activity": "${test()}"
                },
                {
                    "$kind": "Microsoft.AdaptiveDialog",
                    "generator": "subDialog.lg",
                    "triggers": [
                        {
                            "$kind": "Microsoft.OnBeginDialog",
                            "actions": [
                                {
                                    "$kind": "Microsoft.SendActivity",
                                    "activity": "${test()}"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
