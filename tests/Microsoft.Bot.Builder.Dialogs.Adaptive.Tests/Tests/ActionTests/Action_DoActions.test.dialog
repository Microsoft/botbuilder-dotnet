{
  "$type": "Microsoft.Test.Script",
  "dialog": {
    "$type": "Microsoft.AdaptiveDialog",
    "id": "planningTest",
    "recognizer": {
      "$type": "Microsoft.RegexRecognizer",
      "intents": [
        {
          "$type": "Microsoft.IntentPattern",
          "intent": "JokeIntent",
          "pattern": "joke"
        }
      ]
    },
    "triggers": [
      {
        "$type": "Microsoft.OnIntent",
        "intent": "JokeIntent",
        "actions": [
          {
            "$type": "Microsoft.SendActivity",
            "activity": "Why did the chicken cross the road?"
          },
          {
            "$type": "Microsoft.EndTurn",
            "id": "EndTurn"
          },
          {
            "$type": "Microsoft.SendActivity",
            "activity": "To get to the other side"
          }
        ]
      },
      {
        "$type": "Microsoft.OnUnknownIntent",
        "actions": [
          {
            "$type": "Microsoft.IfCondition",
            "condition": "(user.name == null)",
            "actions": [
              {
                "$type": "Microsoft.TextInput",
                "property": "user.name",
                "prompt": "Hello, what is your name?"
              }
            ]
          },
          {
            "$type": "Microsoft.SendActivity",
            "activity": "Hello @{user.name}, nice to meet you!"
          }
        ]
      }
    ],
    "AutoEndDialog": true,
    "defaultResultProperty": "dialog.result"
  },
  "script": [
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "hi"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Hello, what is your name?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "Carlos"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Hello Carlos, nice to meet you!"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "Do you know a joke?"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Why did the chicken cross the road?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "Why?"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "To get to the other side"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "hi"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Hello Carlos, nice to meet you!"
    }
  ]
}