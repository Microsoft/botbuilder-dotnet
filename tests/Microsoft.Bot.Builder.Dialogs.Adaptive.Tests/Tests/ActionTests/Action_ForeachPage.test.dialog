{
  "$type": "Microsoft.Test.Script",
  "dialog": {
    "$type": "Microsoft.AdaptiveDialog",
    "id": "root",
    "triggers": [
      {
        "$type": "Microsoft.OnBeginDialog",
        "event": "beginDialog",
        "actions": [
          {
            "$type": "Microsoft.InitProperty",
            "property": "dialog.todo",
            "type": "Array"
          },
          {
            "$type": "Microsoft.EditArray",
            "itemsProperty": "dialog.todo",
            "value": "1"
          },
          {
            "$type": "Microsoft.EditArray",
            "itemsProperty": "dialog.todo",
            "value": "2"
          },
          {
            "$type": "Microsoft.EditArray",
            "itemsProperty": "dialog.todo",
            "value": "3"
          },
          {
            "$type": "Microsoft.EditArray",
            "itemsProperty": "dialog.todo",
            "value": "4"
          },
          {
            "$type": "Microsoft.EditArray",
            "itemsProperty": "dialog.todo",
            "value": "5"
          },
          {
            "$type": "Microsoft.EditArray",
            "itemsProperty": "dialog.todo",
            "value": "6"
          },
          {
            "$type": "Microsoft.ForeachPage",
            "itemsProperty": "dialog.todo",
            "pageSize": 3,
            "actions": [
              {
                "$type": "Microsoft.SendActivity",
                "activity": "This page have 3 items"
              },
              {
                "$type": "Microsoft.Foreach",
                "itemsProperty": "dialog.foreach.page",
                "actions": [
                  {
                    "$type": "Microsoft.SendActivity",
                    "activity": "index is: @{dialog.foreach.index} and value is: @{dialog.foreach.value}"
                  }
                ]
              }
            ]
          }
        ]
      }
    ],
    "AutoEndDialog": true,
    "defaultResultProperty": "dialog.result"
  },
  "script": [
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "hi"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "This page have 3 items",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "index is: 0 and value is: 1",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "index is: 1 and value is: 2",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "index is: 2 and value is: 3",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "This page have 3 items",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "index is: 0 and value is: 4",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "index is: 1 and value is: 5",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "index is: 2 and value is: 6",
      "exact": true,
      "timeout": 3000
    }
  ]
}