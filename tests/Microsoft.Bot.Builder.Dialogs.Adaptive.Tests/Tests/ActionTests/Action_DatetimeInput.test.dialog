{
  "$type": "Microsoft.Test.Script",
  "dialog": {
    "$type": "Microsoft.AdaptiveDialog",
    "id": "planningTest",
    "triggers": [
      {
        "$type": "Microsoft.OnUnknownIntent",
        
        "actions": [
          {
            "$type": "Microsoft.DateTimeInput",
            "property": "user.date",
            "prompt": "Please enter a date."
          },
          {
            "$type": "Microsoft.SendActivity",
            "activity": "You entered @{user.date[0].Value}"
          }
        ]
      }
    ],
    "defaultResultProperty": "dialog.result"
  },
  "script": [
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "hi"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Please enter a date."
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "June 1st 2019"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "You entered 2019-06-01"
    }
  ]
}