{
  "$type": "Microsoft.Test.Script",
  "dialog": {
    "$type": "Microsoft.AdaptiveDialog",
    "id": "root",
    "recognizer": {
      "$type": "Microsoft.RegexRecognizer",
      "intents": [
        {
          "$type": "Microsoft.IntentPattern",
          "intent": "StartOuterIntent",
          "pattern": "start"
        },
        {
          "$type": "Microsoft.IntentPattern",
          "intent": "RootIntent",
          "pattern": "root"
        }
      ]
    },
    "triggers": [
      {
        "$type": "Microsoft.OnIntent",
        "intent": "StartOuterIntent",
        
        "actions": [
          {
            "$type": "Microsoft.AdaptiveDialog",
            "id": "outer",
            "recognizer": {
              "$type": "Microsoft.RegexRecognizer",
              "intents": [
                {
                  "$type": "Microsoft.IntentPattern",
                  "intent": "SideIntent",
                  "pattern": "side"
                },
                {
                  "$type": "Microsoft.IntentPattern",
                  "intent": "CancelIntent",
                  "pattern": "cancel"
                }
              ]
            },
            "triggers": [
              {
                "$type": "Microsoft.OnBeginDialog",
                
                "actions": [
                  {
                    "$type": "Microsoft.TextInput",
                    "property": "user.name",
                    "prompt": "name?"
                  },
                  {
                    "$type": "Microsoft.SendActivity",
                    "activity": "@{user.name}"
                  },
                  {
                    "$type": "Microsoft.NumberInput",
                    "allowInterruptions": "False",
                    "property": "user.age",
                    "prompt": "age?",
                    "maxTurnCount": 2
                  },
                  {
                    "$type": "Microsoft.NumberInput",
                    "allowInterruptions": "True",
                    "property": "user.age",
                    "prompt": "age?"
                  },
                  {
                    "$type": "Microsoft.SendActivity",
                    "activity": "@{user.age}"
                  }
                ]
              },
              {
                "$type": "Microsoft.OnIntent",
                "intent": "SideIntent",
                
                "actions": [
                  {
                    "$type": "Microsoft.SendActivity",
                    "activity": "sideintent"
                  }
                ]
              },
              {
                "$type": "Microsoft.OnIntent",
                "intent": "CancelIntent",
                
                "actions": [
                  {
                    "$type": "Microsoft.EndDialog"
                  }
                ]
              },
              {
                "$type": "Microsoft.OnUnknownIntent",
                
                "actions": [
                  {
                    "$type": "Microsoft.SendActivity",
                    "activity": "outerWhat"
                  }
                ]
              }
            ],
            "defaultResultProperty": "dialog.result"
          }
        ]
      },
      {
        "$type": "Microsoft.OnIntent",
        "intent": "RootIntent",
        
        "actions": [
          {
            "$type": "Microsoft.SendActivity",
            "activity": "rootintent"
          }
        ]
      },
      {
        "$type": "Microsoft.OnUnknownIntent",
        
        "actions": [
          {
            "$type": "Microsoft.SendActivity",
            "activity": "rootunknown"
          }
        ]
      }
    ],
    "defaultResultProperty": "dialog.result"
  },
  "script": [
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "start"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "name?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "side"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "sideintent"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "name?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "root"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "rootintent"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "name?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "Carlos"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Carlos"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "age?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "root"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "age?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "side"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "age?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "root"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "rootintent"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "age?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "side"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "sideintent"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "age?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "10"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "10"
    }
  ]
}