{
  "$type": "Microsoft.Test.Script",
    "dialog": {
        "$type": "Microsoft.AdaptiveDialog",
        "id": "outer",
        "autoEndDialog": false,
        "recognizer": {
            "$type": "Microsoft.RegexRecognizer",
            "intents": [
                {
                    "$type": "Microsoft.IntentPattern",
                    "intent": "BeginIntent",
                    "pattern": "(?i)begin"
                },
                {
                    "$type": "Microsoft.IntentPattern",
                    "intent": "HelpIntent",
                    "pattern": "(?i)help"
                }
            ]
        },
        "triggers": [
            {
                "$type": "Microsoft.OnBeginDialog",
                "actions": [
                    {
                        "$type": "Microsoft.SendActivity",
                        "activity": "Hi, type 'begin' to start a dialog, type 'help' to get help."
                    }
                ]
            },
            {
                "$type": "Microsoft.OnIntent",
                "intent": "BeginIntent",
                "actions": [
                    {
                        "$type": "Microsoft.TraceActivity"
                    },
                    "AdaptiveDialog_BeingDialog_innerDialog"
                ]
            },
            {
                "$type": "Microsoft.OnIntent",
                "intent": "HelpIntent",
                "actions": [
                    {
                        "$type": "Microsoft.SendActivity",
                        "activity": "help is coming"
                    }
                ]
            },
            {
                "$type": "Microsoft.OnUnknownIntent",
                "event": "unknownIntent",
                "actions": [
                    {
                        "$type": "Microsoft.SendActivity",
                        "activity": "Hi, type 'begin' to start a dialog, type 'help' to get help."
                    }
                ]
            }
        ],
        "defaultResultProperty": "dialog.result"
    },
  "script": [
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "hi"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Hi, type 'begin' to start a dialog, type 'help' to get help.",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "begin"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Hello, what is your name?",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "Carlos"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Hello Carlos, nice to meet you!",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "I'm a joke bot. To get started say 'tell me a joke'",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "tell me a joke"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Why did the chicken cross the road?",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "Why?"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "To get to the other side",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "hi"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Hello Carlos, nice to see you again!",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "Do you know a joke?"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Why did the chicken cross the road?",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "Why?"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "To get to the other side",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "ummm"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "Like I said, I'm a joke bot. To get started say 'tell me a joke'",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "help"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "help is coming",
      "exact": true,
      "timeout": 3000
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "bye"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "See you later aligator!",
      "exact": true,
      "timeout": 3000
    }
  ]
}