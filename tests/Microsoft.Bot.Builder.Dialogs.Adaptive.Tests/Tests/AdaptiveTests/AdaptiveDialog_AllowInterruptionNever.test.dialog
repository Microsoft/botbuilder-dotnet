{
  "$type": "Microsoft.Test.Script",
  "dialog": {
    "$type": "Microsoft.AdaptiveDialog",
    "id": "AdaptiveDialog",
    "recognizer": {
      "$type": "Microsoft.RegexRecognizer",
      "intents": [
        {
          "$type": "Microsoft.IntentPattern",
          "intent": "Start",
          "pattern": "(?i)start"
        }
      ]
    },
    "generator": {
      "$type": "Microsoft.TemplateEngineLanguageGenerator",
      "id": "",
      "lgText": ""
    },
    "triggers": [
      {
        "$type": "Microsoft.OnIntent",
        "intent": "Start",
        "event": "recognizedIntent",
        "actions": [
          {
            "$type": "Microsoft.NumberInput",
            "allowInterruptions": "False",
            "property": "user.age",
            "prompt": "What is your age?"
          },
          {
            "$type": "Microsoft.SendActivity",
            "activity": "I have @{user.age} as your age"
          }
        ]
      },
      {
        "$type": "Microsoft.OnIntent",
        "intent": "None",
        "event": "recognizedIntent",
        "actions": [
          {
            "$type": "Microsoft.SendActivity",
            "activity": "In None..."
          },
          {
            "$type": "Microsoft.SetProperty",
            "property": "turn.processInput",
            "value": "True"
          }
        ]
      }
    ],
    "AutoEndDialog": true,
    "defaultResultProperty": "dialog.result"
  },
  "script": [
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "start"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "What is your age?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "santa"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "What is your age?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "red"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "What is your age?"
    },
    {
      "$type": "Microsoft.Test.UserSays",
      "text": "36"
    },
    {
      "$type": "Microsoft.Test.AssertReply",
      "text": "I have 36 as your age"
    }
  ]
}