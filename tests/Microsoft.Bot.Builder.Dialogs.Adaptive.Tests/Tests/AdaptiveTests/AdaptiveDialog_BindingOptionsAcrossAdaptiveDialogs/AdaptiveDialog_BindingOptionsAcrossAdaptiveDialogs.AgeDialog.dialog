{
  "$type": "Microsoft.AdaptiveDialog",
  "id": "ageDialog",
  "triggers": [
    {
      "$type": "Microsoft.OnUnknownIntent",
      
      "actions": [
        {
          "$type": "Microsoft.SendActivity",
          "activity": "Hello, you are @{dialog.options.userAge} years old!"
        },
        {
          "$type": "Microsoft.SendActivity",
          "activity": "And your actual age is @{$options.userAge}"
        }
      ]
    }
  ],
  "autoEndDialog": true,
  "defaultResultProperty": "dialog.result"
}