
{
    "$schema": "https://raw.githubusercontent.com/microsoft/botbuilder-dotnet/master/schemas/sdk.schema",
    "$kind": "Microsoft.OnAssignEntity",
    "property": "Price",
    "entity": "money",
    "actions": [
        {
            "$kind": "Microsoft.SetProperty",
            "property": "$Price.number",
            "value": "=@money.number"          
        },
        {
            "$kind": "Microsoft.SetProperty",
            "property": "$Price.units",
            "value": "=@money['units']"          
        }
    ]               
}
