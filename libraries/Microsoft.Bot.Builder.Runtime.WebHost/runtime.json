{
  "adapters": [
    {
      "$kind": "Microsoft.BotCoreAdapter",
      "middleware": [
        {
          "$kind": "Microsoft.TranscriptLoggerMiddleware",
          "transcriptStore": {
            "$kind": "Microsoft.BlobsTranscriptStore",
            "connectionString": "=blobStorage.connectionString",
            "containerName": "=blobStorage.container"
          }
        },
        {
          "$kind": "Microsoft.ShowTypingMiddleware",
          "delay": 500,
          "period": 2000
        },
        {
          "$kind": "Microsoft.RemoveRecipientMentionMiddleware"
        }
      ]
    }
  ],
  "channel": {
    "$kind": "Microsoft.DeclarativeChannelProvider",
    "channel": null
  },
  "credentials": {
    "$kind": "Microsoft.DeclarativeCredentialsProvider",
    "applicationId": "=MicrosoftAppId",
    "applicationPassword": "=MicrosoftAppPassword"
  },
  "defaultLocale": "en-US",
  "rootDialog": "=defaultRootDialog",
  "storage": {
    "$kind": "Microsoft.CosmosDbPartitionedStorage",
    "authenticationKey": "=cosmosDb.authKey",
    "compatibilityMode": true,
    "containerId": "=cosmosDb.containerId",
    "containerThroughput": 400,
    "databaseId": "=cosmosDb.databaseId",
    "endpoint": "=cosmosDb.cosmosDBEndpoint",
    "keySuffix": null
  },
  "telemetry": {
    "$kind": "Microsoft.ApplicationInsightsTelemetryProvider",
    "instrumentationKey": "=applicationInsights.InstrumentationKey"
  }
}