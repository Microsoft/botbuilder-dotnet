{
  "$schema": "../../Schemas/app.schema",
  "$type": "Microsoft.RuleDialog",
  "$id": "root",
  "storage": {
    "$type": "Microsoft.MemoryStorage"
  },
  "recognizer": {
    "$type": "Microsoft.RegexRecognizer",
    "rules": {
      "HelpIntent": "help",
      "GreetingIntent": "hi|hello|hiya|greetings",
      "JokeIntent": "joke",
      "FortuneTellerIntent": "fortune|future"
    }
  },
  "rules": [
    {
      "$type": "Microsoft.IntentRule",
      "intent": "JokeIntent",
      "steps": [
        {
          "$type": "Microsoft.CallDialog",
          "dialog": {
            "$ref": "Samples/Planning 8 - ExternalLanguage/TellJokeDialog.dialog"
          }
        }
      ]
    },
    {
      "$type": "Microsoft.IntentRule",
      "intent": "FortuneTellerIntent",
      "steps": [
        {
          "$type": "Microsoft.CallDialog",
          "dialog": {
            "$ref": "Samples/Planning 8 - ExternalLanguage/FortuneTellerDialog.dialog"
          }
        }
      ]
    },
    {
      "$type": "Microsoft.WelcomeRule",
      "steps": [
        {
          "$type": "Microsoft.SendActivity",
          "activity": "[welcome]"
        }
      ]
    },
    {
      "$type": "Microsoft.IntentRule",
      "intent": "GreetingIntent",
      "steps": [
        {
          "$type": "Microsoft.CallDialog",
          "dialog": {
            "$ref": "Samples/Planning 8 - ExternalLanguage/Greeting.dialog"
          }
        }
      ]
    },
    {
      "$type": "Microsoft.IntentRule",
      "intent": "HelpIntent",
      "steps": [
        {
          "$type": "Microsoft.SendActivity",
          "activity": "[help]"
        }
      ]
    },
    {
      "$type": "Microsoft.FallbackRule",
      "steps": [
        {
          "$type": "Microsoft.SendActivity",
          "activity": "[help]"
        }
      ]
    }
  ]
}